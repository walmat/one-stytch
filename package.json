{
  "name": "pygmy",
  "version": "1.1.330",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "$npm_execpath one dev",
    "dev:clean": "one dev --clean",
    "clean": "one clean",
    "prebuild:native": "one prebuild",
    "build:web": "one build",
    "serve": "one serve",
    "ios": "one run:ios",
    "android": "one run:android",
    "upgrade:tamagui": "$npm_execpath up '*tamagui*' '@tamagui/*'",
    "db:init": "$npm_execpath db:generate && $npm_execpath db:push && $npm_execpath db:seed",
    "db:check": "bash ./scripts/db-check.sh",
    "db:generate": "drizzle-kit generate --config ./code/db/drizzle.config.ts",
    "db:push": "drizzle-kit push --config ./code/db/drizzle.config.ts",
    "db:migrate": "dotenvx run -f .env -- tsx ./code/db/run-migrations.ts",
    "db:seed": "dotenvx run -f .env -- tsx ./code/db/seed.ts"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.12.1",
    "@rehookify/datepicker": "^6.6.7",
    "@tamagui/config": "^1.116.14",
    "@tamagui/image-next": "^1.116.14",
    "@tamagui/lucide-icons": "^1.116.14",
    "@ts-react/form": "1.6.4",
    "@vxrn/color-scheme": "1.1.330",
    "better-sqlite3": "^11.2.1",
    "console-browserify": "^1.1.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.24.2",
    "drizzle-orm": "^0.33.0",
    "expo": "~51.0.28",
    "expo-document-picker": "^13.0.1",
    "expo-image-picker": "^16.0.2",
    "expo-modules-core": "^1.12.24",
    "expo-secure-store": "^14.0.0",
    "one": "1.1.330",
    "path-browserify": "0.0.0",
    "postgres": "^3.4.4",
    "process": "^0.11.0",
    "react": "18.2.0",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.53.2",
    "react-native": "0.74.5",
    "react-native-level-fs": "^3.0.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-udp": "^2.1.0",
    "react-native-web": "^0.19.12",
    "tamagui": "^1.116.14",
    "viem": "^2.21.45"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@faker-js/faker": "next",
    "@tamagui/vite-plugin": "^1.116.14",
    "@types/react": "^18.3.11",
    "tsx": "^4.19.0",
    "vite": "^5.0.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}
